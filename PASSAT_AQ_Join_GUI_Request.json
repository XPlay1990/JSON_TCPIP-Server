					"created": "YYYY-MM-DDTHH:mm:ss.sssZ",
					"configuration": {
						"hardware": {
							"iftype": "serial, ethernet",
							"ifconfig": {
							  "portname": "ttyUSB0",
							  "bitrate": 11500,
							  "data_bits": 8,
							  "stop_bits": 1,
							  "parity_bits": "n",
							  "port": 55556,
							  "hostname": "localhost",
							  "target_ip": "192.168.10.20",
							  "eth_name": "eth0"
							}
						},
						"protocol": {
							"prot_name": "mwo", 
							"prot_id": 1,
							"prot_version": "1.2"
						}
					}
				}
			}
		]
	}
}